  object LogQuery: TSQLQuery
    MaxBlobSize = 1
    Params = <>
    SQL.Strings = (
      
        'SELECT `Spectra`.`No`, `Date`, `Organization`.`Name`, `Laborator' +
        'y`.`Name`, `Name`.`LastName`,'
      
        '`NIR`.`Num`, `Task`.`Name`, `Spectra`.`Subst`, `Conditions`, `Us' +
        'er`.`LastName`, `Number`'
      'FROM `User`'
      '  INNER JOIN (`Task`'
      '    INNER JOIN (`Organization`'
      '      INNER JOIN (`NIR`'
      '        INNER JOIN (`Name`'
      '          INNER JOIN (`Laboratory`'
      
        '            INNER JOIN `Spectra` ON `Laboratory`.`ID` = `Spectra' +
        '`.`Laboratory`)'
      '          ON `Name`.`ID` = `Spectra`.`Name`)'
      '        ON `NIR`.`ID` = `Spectra`.`NIR`)'
      '      ON `Organization`.`ID` = `Spectra`.`Organization`)'
      '    ON `Task`.`ID` = `Spectra`.`Task`)'
      '  ON `User`.`ID` = `Spectra`.`User`'
      'ORDER BY `Date`, `Spectra`.`ID`;')
    SQLConnection = DB
    Left = 360
    Top = 192
  end
